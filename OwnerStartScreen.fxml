<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="controller.OwnerStartScreenController"
      alignment="CENTER"
      spacing="10">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Button fx:id="booksButton" text="Books" onAction="#handleBooks"/>
    <Button fx:id="customersButton" text="Customers" onAction="#handleCustomers"/>
    <Button fx:id="logoutButton" text="Logout" onAction="#handleLogout"/>
</VBox>
